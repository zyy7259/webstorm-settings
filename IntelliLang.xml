<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="Vue" injector-id="xml">
      <display-name>router-link/@to</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("to")).withParent(xmlTag().withLocalName(string().equalTo("router-link")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>